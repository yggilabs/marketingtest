---
---
{%- assign array = '' | split: '' -%}
{%- assign impressions = site.data.impressions[page.title] -%}
{%- for date in impressions -%}
{%- assign date_key = date[0] -%}
{%- assign date_value = date[1] -%}
{%- for hour in date_value -%}
{%- assign hour_key = hour[0] -%}
{%- assign hour_value = hour[1] -%}
{%- assign a = '' | split: '' -%}
{%- capture a.time -%}{{ date_key }}T{{ hour_key }}:00Z{%- endcapture -%}
{%- assign a.value = hour_value -%}
{%- assign array = array | push: a -%}
{%- endfor -%}
{%- endfor -%}
{{ array | jsonify }}
